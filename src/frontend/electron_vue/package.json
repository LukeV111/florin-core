{
  "name": "Florin",
  "description": "Florin currency",
  "author": "Centure.com BV",
  "version": "2.0.12",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "node ./setup.js && vue-cli-service lint",
    "electron:build": "node ./setup.js && vue-cli-service electron:build",
    "electron:serve": "node ./setup.js && vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "libunity:copy": "node ./libunity/copy.js",
    "libunity:generate": "node ./libunity/generate.js && yarn lint"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^6.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/pro-light-svg-icons": "^6.2.1",
    "@fortawesome/pro-regular-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^2.0.9",
    "axios": "^1.2.1",
    "core-js": "^3.26.1",
    "electron-better-ipc": "^2.0.1",
    "electron-context-menu": "^3.6.1",
    "electron-store": "^8.1.0",
    "lodash.clonedeep": "^4.5.0",
    "portal-vue": "^2.1.7",
    "qrcode": "^1.5.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-js-toggle-button": "^1.3.3",
    "vue-qrcode": "^2.2.0",
    "vue-router": "^4.1.6",
    "vue-slider-component": "^3.2.23",
    "vuex": "^4.1.0",
    "vuex-electron": "https://github.com/devlz303/vuex-electron.git#v1.0.4"
  },
  "devDependencies": {
    "@oshell/vue-cli-plugin-electron-builder-notarize": "^1.0.2",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "electron": "^12.2.3",
    "electron-builder": "^23.6.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "native-ext-loader": "^2.3.0",
    "prettier": "^2.8.0",
    "vue-cli-plugin-ant-design": "~1.0.1",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.75.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 160
  },
  "browserslist": [
    "Electron >= 17.1.0"
  ]
}
